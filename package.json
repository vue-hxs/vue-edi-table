{
  "name": "vue-edi-table",
  "version": "0.0.14",
  "main": "dist/vue-edi-table.common.js",
  "author": "luis <luisf@hexasoftware.com>",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib src/index.js",
    "build-demo": "NODE_ENV=development vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "css-loader": "^1.0.0",
    "element-ui": "^2.4.9",
    "vue": "^2.5.17",
    "vue-split-layout": "0.0.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.5",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard"
    ],
    "rules": {
      "vue/valid-v-model": 0,
      "no-case-declarations": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
